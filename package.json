{
  "name": "electron-react-ts",
  "private": true,
  "version": "1.0.0",
  "main": "index.ts",
  "homepage": ".",
  "type": "module",
  "scripts": {
    "dev": "vite --port 4000",
    "dev:app": "electron . dev",
    "build": "tsc && vite build",
    "pack:win": "electron-builder",
    "dist1": "electron-builder --win --x64",
    "preview": "vite preview"
  },
  "build": {
    "productName": "music",
    "appId": "com.ele.eletest",
    "copyright": "cdÂ©20211209",
    "directories": {
      "buildResources": "./",
      "output": "pack"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "electron.js"
    ],
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./favicon.ico",
      "uninstallerIcon": "./favicon.ico",
      "installerHeaderIcon": "./favicon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "perMachine": true,
      "shortcutName": "elename"
    },
    "win": {
      "icon": "./favicon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@reduxjs/toolkit": "^1.8.3",
    "antd": "^4.21.6",
    "axios": "^0.27.2",
    "dayjs": "^1.11.4",
    
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "electron-builder": "^23.1.0",
    "electron": "^19.0.8",
    "sass": "^1.53.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  }
}